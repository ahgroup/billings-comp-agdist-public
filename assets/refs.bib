  author = {Sullivan, Sheena G. and Khvorov, Arseniy and Carolan, Louise and Dowson, Leslie and Hadiprodjo, A. Jessica and {Sanchez-Ovando}, Stephany and Liu, Yi and Leung, Vivian K. Y. and Hodgson, David and Blyth, Christopher C. and Macnish, Marion and Cheng, Allen C. and Haugenauer, Michelle and Clark, Julia and Dougherty, Sonia and Macartney, Kristine and Koirala, Archana and Khatami, Ameneh and Jadhav, Ajay and Marshall, Helen and Riley, Kathryn E. and Wark, Peter A. B. and Delahunty, Catherine and Subbarao, Kanta and Kucharski, Adam J. and Fox, Annette},
